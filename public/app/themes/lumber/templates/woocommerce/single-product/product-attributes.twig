<table class="woocommerce-product-attributes shop_attributes">
	{% for  product_attribute_key, product_attribute in product.get_attributes() %}
		<tr class="woocommerce-product-attributes-item woocommerce-product-attributes-item--{{ fn( "esc_attr", product_attribute_key ) }}">
			<th class="woocommerce-product-attributes-item__label">{{ fn("wp_kses_post", product_attribute.label) }}</th>
			<td class="woocommerce-product-attributes-item__value">{{ fn("wp_kses_post", product_attribute.value) }}</td>
		</tr>
	{% endfor %}
</table>
